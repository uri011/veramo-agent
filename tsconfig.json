{
    "compilerOptions": {
        "preserveConstEnums": true,
        "strict": true,
        "target": "ESNext",
        "module": "ESNext",
        "rootDir": "./",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}
